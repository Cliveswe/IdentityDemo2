@model MembersVM
@{
    ViewBag.Title = "Members";
    ViewData["BodyClass"] = "members-bg";
}


<div class="container mt-4">
    <h3 class="text-outline">Members</h3>

    @if (User.Identity!.IsAuthenticated)
    {
        <p class="text-outline">
            Welcome <strong>@User.Identity.Name</strong>!
            <br/>
        </p>
        <p class="text-outline">@Model.FirstName @Model.LastName</p>
    }
    else
    {
        <p class="text-outline">Du är inte inloggad...</p>
    }
</div>